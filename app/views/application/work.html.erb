<% provide(:title, @title) %>

<div class="row">
	<div class="col-6 offset-3">
		<p>My<br>Work</p>
		<% if Example.any? %>
			<% @examples.each do |e| %>
				<%= render 'partials/example', e: e %>
			<% end %>
		<% end %>
		<% if logged_in? %>
			<h1>Add New Example</h1>
			<%= render 'forms/example' %>
		<% end %>
		<p></p>
	</div>
</div>